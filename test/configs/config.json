{
    "environment": "development",
    "debug": {
      "port": "6060"
    },
    "cache": {
      "redis": {
        "marshaller": "proto",
        "compressor": "snappy",
        "addresses": {
          "server": "localhost:6379"
        }
      },
      "ristretto": {
        "num_counters": 10000000,
        "max_cost": 100000000,
        "buffer_items": 64
      }
    },
    "feature": {
      "kind": "flipt",
      "host": "localhost:9000",
      "retry": {
        "timeout": "1s",
        "attempts": 3
      }
    },
    "hooks": {
      "secret": "YWJjZGUxMjM0NQ=="
    },
    "limiter": {
      "kind": "user-agent",
      "pattern": "10-S"
    },
    "sql": {
      "pg": {
        "masters": [
          {
            "url": "postgres://test:test@localhost:5432/test?sslmode=disable"
          }
        ],
        "slaves": [
          {
            "url": "postgres://test:test@localhost:5432/test?sslmode=disable"
          }
        ],
        "max_open_conns": 5,
        "max_idle_conns": 5,
        "conn_max_lifetime": "1h"
      }
    },
    "telemetry": {
      "logger": {
        "level": "info"
      },
      "metrics": {
        "kind": "prometheus"
      },
      "tracer": {
        "kind": "otlp",
        "host": "http://localhost:4318/v1/traces"
      }
    },
    "time": {
      "ntp": {
        "host": "0.beevik-ntp.pool.ntp.org"
      },
      "nts": {
        "host": "time.cloudflare.com"
      }
    },
    "token": {
      "kind": "none"
    },
    "transport": {
      "http": {
        "port": "11000",
        "retry": {
          "timeout": "1s",
          "attempts": 3
        },
        "user_agent": "Service http/1.0"
      },
      "grpc": {
        "port": "12000",
        "retry": {
          "timeout": "1s",
          "attempts": 3
        },
        "user_agent": "Service grpc/1.0"
      }
    }
  }
